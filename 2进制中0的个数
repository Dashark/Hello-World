#include<stdio.h>
#include<math.h>
#include<string.h>
int main()
{
	int n;
	scanf("%d",&n);
	int b[30],c=0,x;
		if(n==0)
	{
		printf("1"); 
	}

	else
	{
		while(n!=0)
		{
			x=n%2;
			b[c]=x;
			c++;
			n=n/2;
		}
		c--;
		int y=0;
		for(;c>=0;--c)
		{
			if(b[c]==0)
			{
				y++;
			}
		}
		printf("%d",y);
		}	
	
}
