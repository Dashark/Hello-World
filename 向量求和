#include<iostream>
using namespace std;
int main(){
	int n,x;
	int a[1000];
	int b[1000];
	while (cin>>n){
		for (int i=0;i<n;i++) {
		cin>>a[i];
		}
		for (int i=0;i<n;i++) {
			cin>>b[i];
		}
		for (int i=0;i<n;i++) {
			x=a[i]+b[i];
							if (i==n-1) cout<<x;
		else cout<<x<<" ";
		}


	}
	return 0;
} 
