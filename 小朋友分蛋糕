#include<iostream>
#include<string>
using namespace std;
int main()
{
	int n=0,k=0;
	int people=0;
	cin>>n>>k;
	int arr[100];
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	for(int m=0;m<n-1;m++)
	{
		int sum=arr[m];
			if(sum<k)
			{
					while(sum<k)
		{
			sum+=arr[m+1];
			m++;
		}
			}
			else 
			{
				
			}
	}
 } 
